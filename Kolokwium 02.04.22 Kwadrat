public class Kwadrat extends Prostokat {

    public Kwadrat(double bok1, double bok2) {
        super(bok1,bok2);
        this.bok1 = bok1;
        this.bok2 = bok2;
    }

    public Kwadrat (double bok1){
        super(bok1 , bok1);
        this.bok1 = bok1;
        this.bok2 = bok1;
    }

    public double przekatna(){
        return (Math.sqrt((bok1 * bok1) + (bok2 * bok2)));
    }

}
